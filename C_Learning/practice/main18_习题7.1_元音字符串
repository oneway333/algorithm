#include "stdio.h"
#include "string.h"

#define N 50

void copy(char str[], int n){
    char out[N] = {0}, pri[]={'a', 'e', 'i', 'o', 'u'};
    int k = 0;
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < 5; ++j) {
            if (str[i] == pri[j]) out[k++] = str[i];
        }
    }
    puts(out);
}

int main() {
    char str[N] = {0};
    gets(str);
    copy(str, strlen(str));
    return 0;
}
