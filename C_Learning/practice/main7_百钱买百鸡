#include <stdio.h>
#include <stdlib.h>
#include "math.h"

int count(int n){
    int a, b, c, res = 0;
    for (a = 0; a <= 100 ; a++) {
        for (b = 0; b <= 100; b++) {
            for (c = 0; c <= 100 ; c++) {
                if ((a + b + c) == 100) {
                    if (c % 3 == 0 && ((5 * a + 3 * b + (c / 3)) == 100)){
                        printf("%d %d %d\n", a , b, c);
                        res++;
                    }
                }
            }
        }
    }

    return res;
}

int main(){
    int n;
    printf("%d", count(100));
    return 0;
}
