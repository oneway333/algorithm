#include<stdio.h>
#include<string.h>

#define N 500

int main(){
    int grade[5][4] = {{10,20,65,23}, {52, 64, 85, 96}, {50,65,78,98}, {89,45,32,75}, {53, 60, 80, 99}};
    double avg[5] = {0};
    for (int i = 0; i < 5; ++i) {
        int s = 0;
        for (int j = 0; j < 4; ++j) s += grade[i][j];
        avg[i] = 1.0 * s / 4;
    }

    for (int i = 0; i < 4; ++i) {
        int k = i;
        for (int j = i + 1; j < 5; ++j) {
            if (avg[k] < avg[j]) k = j;
        }
        if (k != i){
            double t = avg[k];
            avg[k] = avg[i];
            avg[i] = t;
        }
    }
    for (int i = 0; i < 5; ++i) {
        printf("%lf ", avg[i]);
    }
    return 0;
}