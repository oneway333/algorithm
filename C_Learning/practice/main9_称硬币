#include<stdio.h>
#include<string.h>

char left[3][7];//存天平右边硬币    题目中共12个硬币，所以两侧最多各6个
char right[3][7];//存左边
char result[3][7];//存结果即天平右侧的状态
//判断假设是否成立的函数，light为1表示假设假币为轻，否则假设假币为重
int isFake(char c, char weight){
    for (int i = 0; i < 3; ++i) {
        char *pleft, *pright; //指向天平两边的字符串
        if (weight == 'l'){
            pleft = left[i];
            pright = right[i];
        }
        if (weight == 'h'){//如果假设假币是重的，则把称量结果左右对换
            pleft = right[i];//为了后面switch语句写一次，如果假币是重的就对换结果
            pright = left[i];
        }
        switch (result[i][0]) {//天平右边的情况
            case 'u'://右边的轻，那么轻的假币应该出现在右侧天平
                if (strchr(pright, c) == NULL) return 0; break;//如果为NULL说明不符合
            case 'e': //说明假设c为轻的假设是错误的
                if (strchr(pleft, c) || strchr(pright, c)) return 0; break;//平衡，所以假币不能出现在任何一侧
            case 'd':
                if (strchr(pleft, c) == NULL) return 0; break;//右边的重，假币应该出现在左侧
        }
    }
    return 1;
}

int main(){
    for (int i = 0; i < 3; ++i) {
        scanf("%s %s %s", left[i],right[i], result[i]);
    }
    for (int i = 0; i < 3; ++i) {
        printf("%s %s %s\n", left[i],right[i], result[i]);
    }
    for (char c = 'A'; c <= 'L' ; c++) {//题目中共12枚硬币
        if (isFake(c, 'l')){//假设c为轻的假币
            printf("light %c", c);
            break;
        }
        else if (isFake(c, 'h')){//假设c为重的假币
            printf("high %c", c);
            break;
        }
    }
}