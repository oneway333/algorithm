#include "stdio.h"
#include "string.h"

void trans(int a[][3]){
    int n = 3; // 行列数
    for (int i = 0; i < n; ++i) {
        for (int j = i; j < n; ++j) {
            int t = a[i][j];
            a[i][j] = a[j][i];
            a[j][i] = t;
        }
    }
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            printf("%d ", a[i][j]);
        }
        putchar('\n');
    }
}

int main(){
    int a[3][3] = {0};
    for (int i = 0; i < 3; ++i) {
        for (int j = 0; j < 3; ++j) {
            scanf("%d", &a[i][j]);
        }
    }
    for (int i = 0; i < 3; ++i) {
        for (int j = 0; j < 3; ++j) {
            printf("%d ", a[i][j]);
        }
        putchar('\n');
    }
    trans(a);
    return 0;
}
