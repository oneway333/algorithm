#include<stdio.h>
#include<string.h>

#define N 40

int main(){
    int a[] = {1, 3, 2, 5, 4, 6, 8, 10, 9, 0} ,n = 10;
    for (int i = 0; i < n - 1; ++i) {
        for (int j = 0; j < n - i - 1; ++j) {
            if (a[j] > a[j + 1]){
                int t = a[j];
                a[j] = a[j + 1];
                a[j + 1] = t;
            }
        }
    }
    for (int i = 0; i < n; ++i) {
        printf("%d ", a[i]);
    }
    return 0;
}