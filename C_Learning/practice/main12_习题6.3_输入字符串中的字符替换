#include<stdio.h>
#include<string.h>

#define N 500

int main(){
    char str[N], res[N], n;
    int i = 0, j = 0;
    gets(str);
    scanf("%c", &n);
    puts(str);
    while (str[i]){
        if (str[i] == n) {
            i++;
            continue;
        }
        res[j++] = str[i++];
    }
    res[j] = '\0';
    puts(res);
    return 0;
}