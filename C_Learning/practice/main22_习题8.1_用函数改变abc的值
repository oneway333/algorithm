#include <stdio.h>

#define N 50

void out(int *a, int *b, int *c){
    int max = 0, min = 0;
    max = (*a > *b) ? (*a > *c ? *a : *c) : (*b > *c ? *b : *c);
    min = (*a < *b) ? (*a < *c ? *a : *c) : (*b < *c ? *b : *c);
    int mid = *a + *b + *c - max - min;
    *a = max;
    *b = mid;
    *c = min;
}

int main(){
    int a, b, c;
    scanf("%d%d%d", &a, &b, &c);
    out(&a, &b, &c);
    printf("%d %d %d", a, b, c);
    return 0;
}