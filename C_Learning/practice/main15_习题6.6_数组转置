#include<stdio.h>
#include<string.h>

#define N 40

int main(){
    int a[] = {8, 6, 5, 4, 1}, n = 5;
    for (int i = 0; i < (n / 2); ++i) {
        int t = a[i];
        a[i] = a[n - 1 - i];
        a[n - 1 - i] = t;
    }
    for (int i = 0; i < n; ++i) {
        printf("%d ", a[i]);
    }
    return 0;
}