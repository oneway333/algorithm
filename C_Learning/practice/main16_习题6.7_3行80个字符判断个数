#include<stdio.h>
#include<string.h>

#define N 40

int main(){
     char str[3][80];
    for (int i = 0; i < 3; ++i) {
        gets(str[i]);
    }
//    puts(str);
    int upAlph, lowAlph, num, space, other;
    upAlph = lowAlph = num = space = other = 0;
    for (int i = 0; i < 3; ++i) {
        int j = 0;
        while (str[i][j]){
            if (str[i][j] >= 'a' && str[i][j] <= 'z') lowAlph++;
            else if (str[i][j] >= 'A' && str[i][j] <= 'Z') upAlph++;
            else if (str[i][j] >= '0' && str[i][j] <= '9') num++;
            else if (str[i][j] == ' ') space++;
            else other++;
            j++;
        }
    }
    printf("upAlph: %d, lowAlph: %d, num: %d, space: %d, other: %d", upAlph, lowAlph, num, space, other);
    return 0;
}