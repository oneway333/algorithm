#include <stdio.h>
#include <string.h>

typedef struct {
    int num;
    char name[30];
    int grade;
} Student;


int main(){
    Student stu[2];
    for (int i = 0; i < 2; ++i) {
        scanf("%d %s %d", &stu[i].num, stu[i].name, &stu[i].grade);
    }
    if (stu[0].grade > stu[1].grade)
        printf("%d %s %d", stu[0].num, stu[0].name, stu[0].grade);
    else
        printf("%d %s %d", stu[1].num, stu[1].name, stu[1].grade);
    return 0;
}
