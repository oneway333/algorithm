#include <stdio.h>
#include <string.h>

typedef struct {
    char name[30];
    int vote;
} Cand;


int main(){
    Cand cand[3] = {"Tom", 0, "Jerry", 0, "Ben", 0};
    int n = 0;
    scanf("%d", &n);
    for (int i = 0; i < n; ++i) {
        char str[30] = {0};
        scanf("%s", str);
        for (int j = 0; j < 3; ++j) {
            if (strcmp(str, cand[j].name) == 0){
                cand[j].vote += 1;
                break;
            }
        }
    }
    for (int i = 0; i < 3; ++i) {
        printf("%s %d\n", cand[i].name, cand[i].vote);
    }
    return 0;
}
