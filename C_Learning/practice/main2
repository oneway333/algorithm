#include <stdio.h>
#include <stdlib.h>

int main(){
    FILE *fp, *fp1;
    if((fp = fopen("D:/C_Learning/temp/1.txt", "r")) == NULL){
        printf("file 1.txt open failed!");
        exit(0);
    }
    if((fp1 = fopen("data.txt", "w")) == NULL){
        printf("file data.txt open failed");
        exit(0);
    }
    char ch;
    while((ch = fgetc(fp)) != EOF){
        fputc(ch, fp1);
    }
    fclose(fp);
    fclose(fp1);
    return 0;

}