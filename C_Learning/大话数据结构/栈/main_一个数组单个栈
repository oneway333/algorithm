#include <stdio.h>

#define MAXSIZE 1000
#define OK 1
#define ERROR 0

typedef int Status;
typedef int SElemType;

typedef struct {
    SElemType data[MAXSIZE];
    int top; // 栈顶的下标
}SqStack;

// 入栈
Status Push(SqStack *S, SElemType e){
    if (S->top >= MAXSIZE - 1) return ERROR;
    S->top++; // 这一步必须先++，因为top初值为-1的
    S->data[S->top] = e;
    return OK;
}

// 出栈
Status Pop(SqStack *S, SElemType *e){
    if (S->top <= -1) return ERROR;
    *e = S->data[S->top];
    S->top--;
    return OK;
}


void show(SqStack *S){
    int i;
    for (i = 0; i <= S->top; i++)
        printf("%d\t", S->data[i]);
}

int main(){
    SqStack S = {.top = -1};
    int i, e;
    for (i = 0; i < 10; i++)
        Push(&S, i + 1);
    show(&S);
    putchar('\n');
    Pop(&S, &e);
    printf("%d\n", e);
    show(&S);
    return 0;
}