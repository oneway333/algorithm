/*
 *
3.1 全排列
给定一个正整数n，输出1-n之间所有数字的全排列。
输入：3
输出：["123", "132", "213", "231", "312", "321"]
 */

#include <stdio.h>

#define N 500

int path[N];
int visited[N];

// k指向第k个数字main.c
void permutations(int n, int k){
    if (k == 3){
        for (int i = 0; i < n; ++i) printf("%d ", path[i]);
        putchar('\n');
        return;
    }
    for (int i = 0; i < n; ++i) {
        if (visited[i + 1] == 1) continue;
        path[k] = i + 1;
        visited[i + 1] = 1;
        permutations(n, k + 1);
        // 回溯中很牛皮的地方
        visited[i + 1] = 0;
    }
}

int main(){

    int n;
    scanf("%d", &n);
    permutations(n, 0);
    return 0;

}