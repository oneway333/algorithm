/*
 *
 *
 * 输入startdate和days，输出enddate
 *
 *
 *
 */

#include <stdio.h>
#include <stdlib.h>

typedef struct{
    int year, month, day;
}Date;

int months[] = {0, 31, 28, 31, 30, 31, 30, 31, 31, 30, 31, 30, 31};

int isLeap(int year){
    if ((year % 4 == 0 && year % 100 != 0) || year % 400 == 0)
        return 1;
    return 0;
}

int test(int year, int month, int day){
    int leap = isLeap(year);
    int t = year / 1000;
    if (t <= 0 || t > 9) return 0;
    if (month < 1 || month > 12) return 0;
    if (day < 1 || (month != 2 && day > months[month])) return 0;
    if (month == 2 && day > months[month] + leap) return 0;
    return 1;
}

void calculate(Date date, int days){
    int y = date.year, m = date.month, d = date.day;
    for (int i = 0; i < days; i++){
        if (isLeap(y)) months[2] = 29;
        else months[2] = 28;
        if (d + 1 > months[m]){
            if (m + 1 > 12){
                y++;
                m = 1;
                d = 1;
            } else {
                m++;
                d = 1;
            }
        } else {
            d++;
        }
    }
    printf("res: %d-%d-%d", y, m, d);
}

int main(){
    Date date;
    int days;
    scanf("%d-%d-%d %d", &date.year, &date.month, &date.day, &days);
    if (!test(date.year, date.month, date.day) || days < 1){
        printf("not legal input!");
        exit(0);
    }
    calculate(date, days);
    return 0;
}
