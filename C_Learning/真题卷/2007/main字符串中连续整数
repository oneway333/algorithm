/*
 *
连续整数统计
 */

#include <stdio.h>

#define N 50

// 将s[l, r)转化成整数
int num(char s[], int l, int r){
    int n = 0;
    for (int i = l; i < r; i++)
        n = 10 * n + s[i] - '0';
    return n;
}

int main(){
    char s[N];
    // a为存储最终结果的，p为数字个数，r为数字的结束位置
    int a[N] = {0}, p = 0, r;
    gets(s);
    // i为连续整数的起始位置
    for (int i = 0; s[i]; i++){
        if(s[i] >= '0' && s[i] <= '9') {
            r = i + 1;
            while (s[r] >= '0' && s[r] <= '9') r++;
            a[p++] = num(s, i, r);
            // 每次遍历完连续整数，i需要到末尾处
            i = r;
        }
    }
    printf("total %d\n", p);
    for (int i = 0; i < p; i++)
        printf("%d\n", a[i]);
    return 0;
}