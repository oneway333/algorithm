/*
 *
 鞍点
 */

#include <stdio.h>

#define N 50

void findPoint(int m, int n, int a[][N]){
    // 没有找到鞍点
    int flag = 0;
    for (int i = 0; i < m; i++){
        int x = 0, j;
        for (j = 0; j < n; j++)
            if (a[i][j] > a[i][x]) x = j;
        for (j = 0; j < m; j++)
            if (a[j][x] < a[i][x]) break;
        if (j >= m) {
            printf("a[%d][%d] is the point, num is %d", i, x, a[i][x]);
            flag = 1;
        }
    }
    if (flag == 0) {
        printf("no point");
    }
}

int main(){
    int m, n, a[N][N] = {0};
    scanf("%d%d", &m, &n);
    for (int i = 0; i < m; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &a[i][j]);
    findPoint(m, n, a);
    return 0;
}