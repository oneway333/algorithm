/*
 *
 *
 * 鞍点*变
 *
 *
 *
 *
 */

#include <stdio.h>

#define N 50

void input(int a[][N], int n){
    for (int i = 0; i < n; ++i)
        for (int j = 0; j < n; ++j)
            scanf("%d", &a[i][j]);
}

void findPoint(int a[][N], int n){
    // 默认没有鞍点
    int flag = 0;
    for (int i = 0; i < n; ++i) {
        int x = 0, j;
        for (j = 1; j < n; j++)
            if (a[i][j] < a[i][x]) x = j;
        for (j = 0; j < n; j++)
            if (a[j][x] < a[i][x]) break;
        if (j >= n) {
            flag = 1;
            printf("%d(%d, %d)\n", a[i][x], i + 1, x + 1);
        }
    }
    if (!flag) printf("NO FIND!");
}

int main(){
    int n, a[N][N];
    scanf("%d", &n);
    input(a, n);
    findPoint(a, n);
    return 0;
}