/*
 *
 *
 * 切比雪大
 *
 *
 *
 *
 */

#include <stdio.h>
#include <stdlib.h>

#define N 50

double T(int n, double x){
    if (n == 0) return 1;
    if (n == 1) return x;
    return 2 * x * T(n - 1, x) - T(n - 2, x);
}

void save(int n, double x){
    FILE *fp;
    if ((fp = fopen("tdat.txt", "a")) == NULL){
        printf("file open failed!");
        exit(0);
    }
    for (int i = 0; i <= n; i++)
        fprintf(fp, "T%d(%lf) = %lf\n", i, x, T(i, x));
    fclose(fp);
}

int main(){
    int n;
    double x;
    scanf("%d %lf", &n, &x);
    save(n, x);
    return 0;
}
