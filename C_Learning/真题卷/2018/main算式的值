/*
 *
 *
 * 计算算式的值
 *
 *
 *
 *
 */

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

void calculate(double a, double b, double c){
    FILE *fp;
    if ((fp = fopen("data.txt", "a")) == NULL){
        printf("file open failed");
        exit(0);
    }
    double T = (4.25 * (a + b) + log(a + b + sqrt(a + b) + 1 / (a + b))) / (4.25 * c + log(c + sqrt(c) + 1 / c));
    fprintf(fp, "%lf %lf %lf, res: %lf\n", a, b, c, T);
    printf("%lf %lf %lf, res: %lf\n", a, b, c, T);
    fclose(fp);
}

int main(){
    double a, b, c;
    int n = 10;
    for (int i = 0; i < n; i++){
        scanf("%lf %lf %lf", &a, &b, &c);
        calculate(a, b, c);
    }
    return 0;

}
