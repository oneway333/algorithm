/*
 *
 *
 * 自然降序数
 *
 *
 *
 *
 */

#include <stdio.h>

void descend(int n){
    int a, b ,t = n;
    while (n){
        if (n > 0 && n <= 9) break;
        a = n % 10;
        b = n / 10 % 10;
        if (a > b){
            printf("%d is not descend\n", t);
            return;
        }
        n /= 10;
    }
    printf("%d is descend\n", t);
}

int main(){
    int n;
    scanf("%d", &n);
    descend(n);
    return 0;
}